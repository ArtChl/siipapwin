	<queryString><![CDATA[SELECT A.PERIODO,a.clave,B.descripcion1,B.kilos
,SUM(a.saldo) AS SALDO 
,AVG((SELECT X.COSTOP FROM SW_INVENTARIOM X WHERE X.ARTICULO_ID=A.ARTICULIO_ID AND TO_NUMBER(SUBSTR(A.PERIODO,4,4))=X.YEAR AND TO_NUMBER(SUBSTR(A.PERIODO,1,2))=X.MES)) AS PRECIO_U
,(ROUND(SUM(a.saldo) 
*AVG((SELECT X.COSTOP FROM SW_INVENTARIOM X WHERE X.ARTICULO_ID=A.ARTICULIO_ID AND TO_NUMBER(SUBSTR(A.PERIODO,4,4))=X.YEAR AND TO_NUMBER(SUBSTR(A.PERIODO,1,2))=X.MES)),2))
AS COSTO 
FROM SW_SALDOS A
JOIN SW_ARTICULOS B ON(A.ARTICULIO_ID=B.ID)
JOIN SW_SUCURSALES S ON(A.SUCURSAL=S.CLAVE)
WHERE --A.PERIODO='07/2007' AND 
A.SALDO<>0  AND TO_NUMBER(SUBSTR(A.PERIODO,4,4))=$P{YEAR} AND TO_NUMBER(SUBSTR(A.PERIODO,1,2))=$P{MES} 
AND A.SUCURSAL LIKE $P{SUCURSAL} AND A.CLAVE LIKE '%'
GROUP BY A.PERIODO,a.clave,B.descripcion1,B.kilos
ORDER BY A.CLAVE,B.DESCRIPCION1]]></queryString>

