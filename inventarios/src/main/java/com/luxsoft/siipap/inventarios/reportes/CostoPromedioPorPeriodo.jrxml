<jasperReport
		 name="CostoPromedioPorPeriodo"
		 columnCount="1"
		 printOrder="Vertical"
		 orientation="Portrait"
		 pageWidth="612"
		 pageHeight="792"
		 columnWidth="552"
		 columnSpacing="0"
		 leftMargin="30"
		 rightMargin="30"
		 topMargin="20"
		 bottomMargin="20"
		 whenNoDataType="NoPages"
		 isTitleNewPage="false"
		 isSummaryNewPage="false">
	<property name="ireport.scriptlethandling" value="0" />
	<property name="ireport.encoding" value="UTF-8" />
	<import value="java.util.*" />
	<import value="net.sf.jasperreports.engine.*" />
	<import value="net.sf.jasperreports.engine.data.*" />

	<parameter name="AÑO" isForPrompting="true" class="java.lang.Integer"/>
	<parameter name="MES" isForPrompting="true" class="java.lang.Integer"/>
	<parameter name="CLAVE" isForPrompting="true" class="java.lang.String"/>
	<parameter name="COMPANY" isForPrompting="false" class="java.lang.String"/>
	<queryString><![CDATA[SELECT B.ARTCLAVE,B.ARTNOMBR,B.CANTIDAD,B.UNIDAD
,B.NETO*C.TC AS NETOMN,B.CANTIDAD*B.NETO*C.TC AS IMPORTE,A.SUCURSAL,A.COM,A.FENT AS FECHA,A.INGRESADA
,'CXP:'||C.FACTURA AS FACTURA,'NO' AS MAQUILA,0 AS CANT_INI,0 AS COSTO_INI,0 AS CANT_FIN,0 AS COSTO_FIN
FROM SW_COMS2 A
JOIN SW_ANALISISDET B ON(A.COM_ID=B.COM_ID)
JOIN SW_ANALISIS C ON(B.ANALISIS_ID=C.ANALISIS_ID)
WHERE TO_NUMBER(TO_CHAR(A.FENT,'YYYY'))=$P{AÑO}  AND TO_NUMBER(TO_CHAR(A.FENT,'MM'))=$P{MES}  AND A.CLAVE=$P{CLAVE}
UNION
--SQL ANALISIS DEL COSTO POR ARTICULO MQH
SELECT E.CLAVE,E.DESCRIPCION1,A.CANTIDAD,B.UNIDAD
,(C.COSTO+(ABS(E.KILOS*A.CANTIDAD*D.PRECIO_HOJ)/a.cantidad)+(ABS(E.KILOS*A.CANTIDAD*A.PRECIOPORKILOFLETE)/a.cantidad)) AS NETOMN
,(C.COSTO+(ABS(E.KILOS*A.CANTIDAD*D.PRECIO_HOJ)/a.cantidad)+(ABS(E.KILOS*A.CANTIDAD*A.PRECIOPORKILOFLETE)/a.cantidad))*A.CANTIDAD AS IMPORTE
,SUCURSAL,COM,FENT,INGRESADA,'MQH:'||X.FACTURA,'SI',0 AS CANT_INI,0 AS COSTO_INI,0 AS CANT_FIN,0 AS COSTO_FIN
from SW_MOVI_MAQUILA D1
LEFT JOIN SW_ANALISIS_MAQ X ON(X.ANALISIS_ID=D1.ANALISIS_ID)
LEFT JOIN SW_MOVI_MAQUILA D on(D.entrada=D1.movimiento_id)
JOIN SW_HOJEADO C ON(C.CORTE_ID=D.MOVIMIENTO_ID)
JOIN SW_HOJEADO A ON(C.HOJEADO_ID=A.ORIGEN_ID)
JOIN SW_ARTICULOS E ON(E.ID=C.ARTICULO_ID)
JOIN SW_COMS2 B ON(B.COM_ID=A.COM_ID)
WHERE TO_NUMBER(TO_CHAR(B.FENT,'YYYY'))=$P{AÑO} AND TO_NUMBER(TO_CHAR(B.FENT,'MM'))=$P{MES} AND B.CLAVE=$P{CLAVE}
UNION
--SQL ANALISIS DEL COSTO POR ARTICULO MQB
SELECT E.CLAVE,E.DESCRIPCION1,-D.KILOS,B.UNIDAD
,ROUND(D1.PKILO+NVL(ABS(D.KILOS*D.PRECIO_HOJ),0),2) AS NETOMN
,ROUND((D1.PKILO+NVL(ABS(D.KILOS*D.PRECIO_HOJ),0))*-D.KILOS,2) AS IMPORTE
,SUCURSAL,COM,FENT,INGRESADA,'MQB:'||X.FACTURA,'SI',0 AS CANT_INI,0 AS COSTO_INI,0 AS CANT_FIN,0 AS COSTO_FIN
from SW_MOVI_MAQUILA D1
LEFT JOIN SW_ANALISIS_MAQ X ON(X.ANALISIS_ID=D1.ANALISIS_ID)
LEFT JOIN SW_MOVI_MAQUILA D on(D.entrada=D1.movimiento_id AND D.TIPO='B')
JOIN SW_ARTICULOS E ON(E.ID=D.BOBINA_ID)
JOIN SW_COMS2 B ON(B.COM_ID=D.COM_ID)
WHERE TO_NUMBER(TO_CHAR(B.FENT,'YYYY'))=$P{AÑO}  AND TO_NUMBER(TO_CHAR(B.FENT,'MM'))=$P{MES}  AND B.CLAVE=$P{CLAVE}
UNION
SELECT C.CLAVE,C.DESCRIPCION1,0 AS CANTIDAD,D.CLAVE
,ROUND(B.COSTOINI/B.INICIAL,2) AS COSTO
,0 AS IMPORTE
,0,0
,TO_DATE('','DD/MM/YYYY')
,0,'INV','NO',B.INICIAL,B.COSTOINI,B.SALDO,B.COSTO
FROM SW_INVENTARIOM B 
JOIN SW_ARTICULOS C ON(C.ID=B.ARTICULO_ID)
JOIN SW_UNIDADES D ON(D.UNIDAD_ID=C.UNIDAD_ID)
WHERE B.YEAR=$P{AÑO} AND B.MES=$P{MES} 
AND B.CLAVE='SBS19016']]></queryString>

