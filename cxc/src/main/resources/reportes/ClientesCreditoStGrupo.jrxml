	<variable name="SUM_CLAVE_1" class="java.lang.String" resetType="Report" calculation="Sum">
		<variableExpression><![CDATA[$F{CLAVE}]]></variableExpression>
	</variable>
	<variable name="SALD_TOTAL" class="java.math.BigDecimal" resetType="Report" calculation="Sum">
		<variableExpression><![CDATA[$F{SALD_XXX}]]></variableExpression>
	</variable>
	<variable name="SLD_VENC" class="java.math.BigDecimal" resetType="Report" calculation="Sum">
		<variableExpression><![CDATA[$F{SALDO_VENC}]]></variableExpression>
	</variable>
	<variable name="SLD_VENCER" class="java.math.BigDecimal" resetType="Report" calculation="Nothing">
		<variableExpression><![CDATA[$F{SALD_XXX}.subtract($F{SALDO_VENC})]]></variableExpression>
	</variable>
	<variable name="PAG_SEM_A" class="java.math.BigDecimal" resetType="Group" resetGroup="STATUS" calculation="Sum">
		<variableExpression><![CDATA[$F{PAG_SEM_A}]]></variableExpression>
	</variable>
	<variable name="SALDO" class="java.math.BigDecimal" resetType="Group" resetGroup="STATUS" calculation="Sum">
