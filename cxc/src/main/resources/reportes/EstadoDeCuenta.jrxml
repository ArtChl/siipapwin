	</parameter>
	<parameter name="FECHA" isForPrompting="true" class="java.util.Date"/>
	<parameter name="CLIENTE" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[select A.SUCURSAL,A.TIPO,A.NUMERO,A.NUMEROFISCAL,A.FECHA,B.FECHAREVISIONCXC,A.CLIENTE,A.NOMBRE
,CASE WHEN TP_DOC='FAC' THEN B.VENCIMIENTO ELSE A.FECHA+(SELECT B.PLAZO FROM SW_CLIENTES B WHERE A.CLIENTE=B.CLAVE)END AS VENCIMIENTO
,A.TOTAL,A.DESCTO,A.PAGOS,A.SALDO
,CASE WHEN A.TP_DOC='FAC' THEN ROUND(TO_DATE($P{FECHA},'DD/MM/YYYY')-B.VENCIMIENTO,0) ELSE ROUND(TO_DATE($P{FECHA},'DD/MM/YYYY')-A.FECHA+(SELECT B.PLAZO FROM SW_CLIENTES B WHERE A.CLIENTE=B.CLAVE),0) END AS ATRASO
,CASE WHEN (CASE WHEN A.TP_DOC IN('FAC') THEN ROUND(TO_DATE($P{FECHA},'DD/MM/YYYY')-B.VENCIMIENTO,0) ELSE ROUND(TO_DATE($P{FECHA},'DD/MM/YYYY')-A.FECHA+(SELECT B.PLAZO FROM SW_CLIENTES B WHERE A.CLIENTE=B.CLAVE),0) END )>0 THEN
((ABS(A.TOTAL)
 -(SELECT NVL(SUM(X.DESCTO),0) FROM V_MOV_VTA X WHERE X.TP_DOC='NCD'  AND X.ID=A.ID AND X.FECHA<=$P{FECHA}) --AS DESCTO
-((SELECT NVL(SUM(X.IMPORTE),0) FROM SW_PAGOS X WHERE X.ORIGEN='CRE' AND X.VENTA_ID=A.ID AND X.FECHA<=$P{FECHA})+ -- PAG_FAC
(SELECT NVL(SUM(X.IMPORTE),0) FROM SW_PAGOS X WHERE X.ORIGEN='CRE' AND X.NOTAPAGO_ID=A.ID AND X.FECHA<=$P{FECHA})))*(A.TOTAL/ABS(A.TOTAL)))
 ELSE 0 END AS VENCIDO
,CASE WHEN (CASE WHEN A.TP_DOC IN('FAC') THEN ROUND(TO_DATE($P{FECHA},'DD/MM/YYYY')-B.VENCIMIENTO,0) ELSE ROUND(TO_DATE($P{FECHA},'DD/MM/YYYY')-A.FECHA+(SELECT B.PLAZO FROM SW_CLIENTES B WHERE A.CLIENTE=B.CLAVE),0) END )<=0 THEN
((ABS(A.TOTAL)
 -(SELECT NVL(SUM(X.DESCTO),0) FROM V_MOV_VTA X WHERE X.TP_DOC='NCD'  AND X.ID=A.ID AND X.FECHA<=$P{FECHA}) --AS DESCTO
-((SELECT NVL(SUM(X.IMPORTE),0) FROM SW_PAGOS X WHERE X.ORIGEN='CRE' AND X.VENTA_ID=A.ID AND X.FECHA<=$P{FECHA})+ -- PAG_FAC
(SELECT NVL(SUM(X.IMPORTE),0) FROM SW_PAGOS X WHERE X.ORIGEN='CRE' AND X.NOTAPAGO_ID=A.ID AND X.FECHA<=$P{FECHA})))*(A.TOTAL/ABS(A.TOTAL)))
 ELSE 0 END AS POR_VENCER
from V_MOV_VTA A
LEFT JOIN SW_VENTASCREDITO B ON(B.VENTA_ID=A.VENTA_ID)
where YEAR>2005 AND SALDO NOT BETWEEN -0.1 AND 0.1 AND a.origen='CRE' AND
TP_DOC not IN('PAG','NCD') AND CLIENTE LIKE $P{CLIENTE} AND FECHA<=$P{FECHA}
ORDER BY A.FECHA,NUMERO,TIPO]]></queryString>

